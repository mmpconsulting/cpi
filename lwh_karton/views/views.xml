<odoo>
  <data>
    <record model="ir.ui.view" id="lwh_form">
      <field name="name">lwh_form.form</field>
      <field name="model">product.template</field>
      <field name="inherit_id" ref="product.product_template_form_view"/>
      <field name="arch" type="xml">
        <xpath expr="//group[@name='group_standard_price']" position="after">
          <group string="Other Dimensions">
            <field name="checkbox_karton"/>
            <field name="jenis_karton" can_create="true" can_write="true" attrs="{'invisible': [('checkbox_karton', '!=', True)]}"/>
            <field name="warna_karton" can_create="true" can_write="true" attrs="{'invisible': [('checkbox_karton', '!=', True)]}"/>
            <field name="isi_karton" attrs="{'invisible': [('checkbox_karton', '!=', True)]}"/>
            <field name="lenght_karton"/>
            <field name="width_karton"/>
            <field name="height_karton"/>
        </group>
        </xpath>
      </field>
    </record>
  </data>
</odoo>