<odoo>
    <record id="outstanding_purchase_order_report_action" model="ir.actions.report">
        <field name="name">Outstanding Report</field>
        <field name="model">outstanding.report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">outstanding_report.report_outstanding_template</field>
        <field name="report_file">outstanding_report.report_outstanding_template</field>
        <field name="print_report_name">'Outstanding Report'</field>
        <field name="paperformat_id" ref="base.paperformat_euro" />
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>

    <record id="outstanding_purchase_order_report_action_so" model="ir.actions.report">
        <field name="name">Outstanding Report Sale Order</field>
        <field name="model">outstanding.report.so</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">outstanding_report.report_outstanding_template_so</field>
        <field name="report_file">outstanding_report.report_outstanding_template_so</field>
        <field name="print_report_name">'Outstanding Report'</field>
        <field name="paperformat_id" ref="base.paperformat_euro" />
        <field name="binding_model_id" eval="False"/>
        <field name="binding_type">report</field>
    </record>
</odoo>